#include <iostream>
#include <string>
#include <vector>
#include <algorithm>


int main()
{
	int k, d;
	long int n;
	std::cin >> n >> k >> d;

	long int fn = n;
	std::string ans;
	for (int day = 1; day <= d; day++)
	{
		n *= 10;
		for (int i = 0; i < 10; i++)
		{
			fn = n + i;
			if (fn%k==0){
				n += i;
				ans = std::to_string(n);
				std::string temp(d-1,'0');
				ans += temp;
				std::cout << ans;
				break;
			}
		}
		if (n % k != 0) { 
			ans = "-1";
			std::cout << ans;
			break;
		}
    
		if (ans != "") {
			break;
		}
	}
}
